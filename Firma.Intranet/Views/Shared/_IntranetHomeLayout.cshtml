@{
    Layout = "~/Views/Shared/_Layout.cshtml"; 
}

<div class="container-fluid mt-4">
    <div class="row g-3">
        <div class="col-md-4">
            @RenderSection("NajblizszeRezerwacje", required: false)
        </div>
        <div class="col-md-8">
            @RenderSection("Wiadomosci", required: false)
        </div>
    </div>

    <div class="row g-3 mt-2">
        <div class="col-md-6">
            @RenderSection("Opoznienia", required: false)
        </div>
        <div class="col-md-6">
            @RenderSection("ProsbyOWypozyczenie", required: false)
        </div>
    </div>

    <div class="row g-3 mt-2">
        <div class="col-md-4">
            @RenderSection("NajnowszySprzet", required: false)
        </div>
        <div class="col-md-4">
            @RenderSection("StatystykaUzytkownikow", required: false)
        </div>
        <div class="col-md-4">
            @RenderSection("Alerty", required: false)
        </div>
    </div>
</div>
